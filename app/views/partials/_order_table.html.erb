<table class="table table-striped no-margin sources-table">
<thead>
<tr>
	<th>#</th>
	<th>ID</th>
	<th>Quantity</th>
	<th>Price</th>
	<th>Status</th>
	<th>Time Submitted</th>
</tr>
</thead>
<tbody>
<% orders.each_with_index do |order, index| %>
	<tr>
	<td><%= index + 1 %></td>
	<% cache order do %>
	<td><%= order.id %></td>
	<td><%= order.quantity %></td>
	<td><%= order.price %></td>
	<td><%= order.status %></td>
	<td><%= order.created_at %></td>
	</tr>
	<% end %>
<% end %>
</tbody>
</table>