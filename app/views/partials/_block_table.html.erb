<table class="table table-striped no-margin sources-table">
<thead>
<tr>
	<th>#</th>
	<th>Transfer ID</th>
	<th>Block Hash</th>
	<th>Coins</th>
	<th>Maturity</th>
	<th>Time Found</th>
</tr>
</thead>
<tbody>
<% @round.blocks.each_with_index do |block, index| %>
	<tr>
	<td><%= index + 1 %></td>
	<% cache block do %>
	<td><%= block.txid %></td>
	<td><%= block.blockhash %></td>
	<td><%= block.amount_string %></td>
	<td><%= block.status_string %></td>
	<td><%= block.time %></td>
	</tr>
	<% end %>
<% end %>
</tbody>
</table>