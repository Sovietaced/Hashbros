<% title "My Payouts" %>
<div class="content container">
	<div class="row">
		<div class="col-md-12">
			<h2 class="page-title">Your Payouts</h2>
		</div>
	</div>
	<div class="row">
		<div class="col-md-12">
			<section class="widget">
				<header>
					<h4>Your Payout History</h4>
				</header>
				<div class="body">
					<% if @payouts.blank? %>
					<p>You do not have any payouts</p>
					<% else %>
						<%= render "partials/payout_table", :payouts => @payouts %>
					<% end %>
				</div>
			</section>
		</div>
	</div>
</div>
